{
    "mapping":"rds20140815/CreateDBInstance",
    "configPackage":"@alicloud/rpc-client",
    "param":{
        "class":{
            "mapping":"DBInstanceClass",
            "required":true
        },
        "net-type":{
            "mapping":"DBInstanceNetType",
            "required":true,
            "choices":[
                "Internet",
                "Intranet"
            ]
        },
        "storage":{
            "mapping":"DBInstanceStorage",
            "required":true,
            "vType":"number"
        },
        "engine":{
            "mapping":"engine",
            "required":true,
            "choices":[
                "MySQL",
                "SQLServer",
                "PostgreSQL",
                "PPAS",
                "MarlaDB"
            ]
        },
        "engine-version":{
            "mapping":"engineVersion",
            "required":true
        },
        "region":{
            "mapping":"RegionId",
            "required":true
        },
        "ip-list":{
            "mapping":"securityIPList",
            "required":true
        },
        "pay-type":{
            "mapping":"payType",
            "required":true,
            "choices":[
                "Postpaid",
                "Prepaid"
            ]
        }
    }
}