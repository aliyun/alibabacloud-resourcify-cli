{
    "long":{
        "zh":"查询某一可用区的资源列表。您可以在某一可用区创建实例或者修改实例规格时查询该可用区的资源列表。"
    },
    "mapping":"ecs20140526/DescribeAvailableResource",
    "configPackage":"@alicloud/rpc-client",
    "param":{
        "resource":{
            "mapping":"destinationResource",
            "required":true,
            "choices":[
                "Zone",
                "IoOptimized",
                "InstanceType",
                "SystemDist",
                "DataDisk",
                "Network",
                "ddh"
            ]
        },
        "region":{
            "mapping":"regionId",
            "required":true,
            "shortHand":"r"
        },
        "zone":{
            "mapping":"zoneId"
        }
    }
}